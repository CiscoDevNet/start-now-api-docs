  "paths": {
    "yourfavoriteDALastName": {
      "get": {
        "description": "Returns your favorite DA Last Name given their first name",
        "operationId": "yourfavoriteDALastName",
        "parameters": [
          {
            "name": "favoriteDA1",
            "in": "query",
            "type": "string",
            "required": true
          },
          {
            "name": "favoriteDA2",
            "in": "query",
            "type": "string",
            "required": true
          }
        ],
        "responses": {
          "200": {
            "description": "Successful operation",
            "schema": {
              "type": "object",
              "x-is-dynamic": true
            },
            "examples": {
              "application/json": {"developer_advocates":[{"first_name":"Kareem","last_name":"Iskander"}, 
                {"first_name":"Matt", "last_name":"DeNapoli"}]}
          }
        },
        "summary": "Returns Favorite DA Last Name",
        "tags": [
          "Favorite DA"
        ]
      }
    }
   }
