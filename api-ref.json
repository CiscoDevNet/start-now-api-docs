{
  "swagger": "2.0",
  "info": {
    "version": "1.0",
    "title": "Start Now Challenge API",
    "description": "The Start Now Challenge API allows is a lookup for your favorite DA's last name.\n"
  },
  "host": "startnowchallenge.cisco.comcom",
  "basePath": "/api/v0",
  "schemes": [
    "https"
  ],
  "consumes": [
    "application/json"
  ],
  "produces": [
    "application/json"
  ],
  "paths": {
    "/yourFavoriteDALastName": {
      "get": {
        "description": "Returns live state from camera of analytics zones",
        "operationId": "getDeviceCameraAnalyticsLive",
        "parameters": [
          {
            "name": "favoriteDA1",
            "description": "Valid values are 'Matt' or 'Kareem'",
            "in": "query",
            "type": "string",
            "required": true
          },
          {
            "name": "favoriteDA2",
            "description": "Valid values are 'Matt' or 'Kareem'",
            "in": "query",
            "type": "string",
            "required": true
          },
          {
            "name": "Content-Type",
            "description": "Request body content type",
            "default": "application/json",
            "required": true,
            "type": "string",
            "in": "header"
          },
          {
            "name": "Authorization",
            "description": "String composed of “Basic”, followed by a space, followed by the Base64 encoding of “username:password”, NOT including the quotes. For example “Basic YWRtaW46TWFnbGV2MTIz”, where YWRtaW46TWFnbGV2MTIz is the Base 64 encoding.",
            "default": "ZGV2bmV0OmlzYXdlc29tZQ==",
            "required": true,
            "type": "string",
            "in": "header"
          }
        
        ],
        "responses": {
          "200": {
            "description": "Successful operation",
            "schema": {
              "type": "object",
              "x-is-dynamic": true
            },
            "examples": {
              "application/json": {"developer_advocates":[{"first_name":"Kareem","last_name":"Iskander"}, 
                {"first_name":"Matt", "last_name":"DeNapoli"}]}}
            }
          }
        },
        "summary": "Returns your favorite DAs Last Name",
        "tags": [
          "Favorite DA"
        ]
      }
    }
  }
}
