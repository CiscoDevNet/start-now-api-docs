{
  "swagger": "2.0",
  "info": {
    "version": "1.0",
    "title": "Start Now Challenge API",
    "description": "The Start Now Challenge API sends your favorite Developer Advocate's Last Name\n"
  }
  "host": "startnowchallenge.cisco.com",
  "basePath": "/api/v0",
  "schemes": [
    "https"
  ],
  "consumes": [
    "application/json"
  ],
  "produces": [
    "application/json"
  ],
  "securityDefinitions": {
    "Basic": {
      "type": "Basic",
      "name": "X-Cisco-Meraki-API-Key",
